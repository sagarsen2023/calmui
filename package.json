{
  "name": "calmui",
  "description": "A CLI tool for generating and managing a consistent project structure for frontend applications.",
  "version": "1.0.2",
  "type": "module",
  "bin": {
    "calmui": "bin/index.js"
  },
  "scripts": {
    "copy-templates": "node --no-warnings --loader ts-node/esm src/scripts/copy-templates.ts",
    "build": "tsc && tsc-esm-fix dist/**/*.js && npm run copy-templates",
    "local-install": "npm run build && npm link && npm i -g .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "frontend",
    "project-structure",
    "scaffolding",
    "react",
    "nextjs",
    "vite",
    "typescript",
    "framework",
    "boilerplate"
  ],
  "author": "Sagar Sen <sagarsen.dev@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/sagarsen2023/calmui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sagarsen2023/calmui.git"
  },
  "bugs": {
    "url": "https://github.com/sagarsen2023/calmui/issues"
  },
  "dependencies": {
    "chalk": "^5.5.0",
    "commander": "^14.0.0",
    "fs-extra": "^11.3.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.3.0",
    "ts-node": "^10.9.2",
    "tsc-esm-fix": "^3.1.2",
    "typescript": "^5.9.2"
  }
}
